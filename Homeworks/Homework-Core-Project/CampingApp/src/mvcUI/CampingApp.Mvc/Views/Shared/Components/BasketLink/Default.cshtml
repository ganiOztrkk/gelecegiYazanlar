@model ProductCollectionCart
@if (Model == null)
{
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Shopping" asp-action="Index">Chart</a>
    </li>
}
else
{
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Shopping" asp-action="Index">Chart <span class="badge rounded-pill bg-black">@Model.ProductItems.Count</span></a>
    </li>
}